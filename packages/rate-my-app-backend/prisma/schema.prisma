datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id      Int      @default(autoincrement()) @id
  email   String   @unique
  name    String
  login   String
  bio   String
  createdAt DateTime @default(now())
  avatarUrl String
}