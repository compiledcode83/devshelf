{
  "name": "www",
  "author": "olafsulich",
  "version": "1.0.1",
  "license": "AGPL-3.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --watch",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "tsc": "tsc --pretty --noEmit",
    "test-all": "npm run lint && npm run type-check && npm run test",
    "analyze": "cross-env ANALYZE=1 next build && cat .next/stats.txt",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "docker": "docker-compose up -d",
    "docker:build": "docker-compose build"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "@devshelf/types": "file:../types",
    "@sentry/browser": "^5.21.4",
    "@sentry/integrations": "^5.21.4",
    "@sentry/node": "^5.21.4",
    "@sentry/webpack-plugin": "^1.12.1",
    "@zeit/next-source-maps": "0.0.3",
    "classnames": "^2.2.6",
    "next": "^10.0.0",
    "next-compose-plugins": "^2.2.0",
    "next-optimized-images": "^2.6.2",
    "next-pwa": "^3.1.3",
    "next-seo": "^4.7.3",
    "normalize-scss": "^7.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "^2.25.2",
    "react-query-devtools": "^2.6.0",
    "webp-loader": "^0.6.0",
    "what-input": "^5.2.10"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@testing-library/cypress": "^7.0.1",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^10.0.1",
    "@types/classnames": "^2.2.10",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.8",
    "@types/testing-library__cypress": "^5.0.8",
    "babel-jest": "^25.2.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-preset-react-app": "^9.1.2",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "cypress": "^5.0.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-jest-dom": "^3.2.4",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-app": "^6.2.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-testing-library": "^3.9.2",
    "identity-obj-proxy": "^3.0.0",
    "jest-watch-typeahead": "^0.5.0",
    "msw": "^0.21.3",
    "next-sitemap": "^1.1.4",
    "react-axe": "^3.5.3",
    "react-is": "^16.13.1",
    "sass": "^1.26.10",
    "ts-jest": "^26.4.2",
    "typescript": "^4.2.3",
    "typescript-plugin-css-modules": "^2.4.0",
    "webpack-bundle-size-analyzer": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint",
      "npm run format"
    ]
