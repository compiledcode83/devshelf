// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Book {
  id    Int    @id @default(autoincrement())
  title String
}

model User {
  id        String   @id
  email     String
  createdAt DateTime  @default(now())
  avatarUrl String
  books     Book[]
}